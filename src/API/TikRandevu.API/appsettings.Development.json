{
  "ConnectionStrings": {
    "Database": "Host=localhost;Port=5432;Database=TikRandevu;Username=postgres;Password=pass;Include Error Detail=true",
    "Redis": "tikrandevu.redis:6379,abortConnect=false"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://tikrandevu.seq:5341" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "tikrandevu.Api"
    }
  }
}
